  "scripts": {
    "build": "babel src --out-dir lib",
    "build:watch": "babel src --out-dir lib --watch",
    "build:single": "babel src/index.js --out-file lib/compiled.js",
    "build:single:watch": "babel src/index.js --out-file lib/compiled.js --watch",
    "clean": "rm -rf lib",
    "dev": "npm run clean && npm run build:watch",
    "test": "node lib/compiled.js",
    "start": "npm run build && npm run test"
  },
